<Window x:Class="WellPt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WellPt"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Window.Resources>
        <Storyboard x:Key="sbAnimateImage">
            <DoubleAnimation Storyboard.TargetName="image" Storyboard.TargetProperty="(Canvas.Left)" By="100" Duration="0:0:2" />
        </Storyboard>
    </Window.Resources>
    <Grid>
        <Canvas>
            <Image x:Name="image" Height="100" Canvas.Top="20" Canvas.Left="20" Source="img/Picture2.png"/>
        </Canvas>
        <Button Content="Button" HorizontalAlignment="Left" Margin="73,184,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
